<h1>Editing Course #<%= @course.id %></h1>

<form action="/courses/<%= @course.id %>/update">
  <div>
    <label for="name">name</label><br />
    <input id="name" name="name" value="<%= @course.name  %>" type="text" />
  </div>
  <div>
    <label for="desc">desc</label><br />
    <input id="desc" name="desc" value="<%= @course.desc  %>" type="text" />
  </div>
  <div>
    <input type="submit">
  </div>
</form>


